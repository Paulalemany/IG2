//__________CIELO_____________________________
material cieloSpace{
	technique{
		pass{
			lighting off
			scene_blend alpha_blend
			depth_write off
			diffuse vertexcolour

			//Textura principal
			texture_unit{	
				texture spacesky.jpg
				colour_op modulate
				scroll_anim <1 (1)> <0 (1)>
			}

			//Textura secundaria
			texture_unit{s
				texture flare.png
				rotate 1.57 
				colour_op add
			}
		}
	}
}